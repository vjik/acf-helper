{
    "name": "vjik/acf-helper",
    "type": "library",
    "description": "Helper for ACF",
    "keywords": [
        "wordpress",
        "acf"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/vjik/acf-helper/issues?state=open",
        "source": "https://github.com/vjik/acf-helper"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "8.1 - 8.4",
        "vjik/simple-type-caster": "^0.4.5",
        "yiisoft/arrays": "^3.0",
        "yiisoft/strings": "^2.0"
    },
    "require-dev": {
        "johnpbloch/wordpress-core": "^5.9.10",
        "phpunit/phpunit": "^10.5.46",
        "vimeo/psalm": "^5.26.1"
    },
    "autoload": {
        "psr-4": {
            "Vjik\\AcfHelper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vjik\\AcfHelper\\Tests\\": "tests"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true
    }
}
